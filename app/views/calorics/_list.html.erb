<div id="first">
  <h4>Today's Calories</h4>

    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Number</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @calorics.where(date: Date.today).each do |caloric| %>
          <tr>
            <td><%= caloric.date %></td>
            <td><%= caloric.number %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
