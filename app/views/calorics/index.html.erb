<p id="notice"><%= notice %></p>

<h1>Listing Calorics</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Numer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calorics.each do |caloric| %>
      <tr>
        <td><%= caloric.date %></td>
        <td><%= caloric.numer %></td>
        <td><%= link_to 'Show', caloric %></td>
        <td><%= link_to 'Edit', edit_caloric_path(caloric) %></td>
        <td><%= link_to 'Destroy', caloric, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caloric', new_caloric_path %>
