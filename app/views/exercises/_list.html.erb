<div id="second">

  <h4>Today's Exercises</h4>

  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Type</th>
        <th>Duration</th>
        <th>Calories Burned</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @exercises.each do |e| %>
        <tr>
          <td><%= e.date %></td>
          <td><%= e.exercise_type_id %></td>
          <td><%= e.duration %></td>
          <td><%= e.calories_burned %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
