<div id="third">
  <h4>Today's Steps</h4>

  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Number</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @steps.each do |step| %>
        <tr>
          <td><%= step.date %></td>
          <td><%= step.number %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
