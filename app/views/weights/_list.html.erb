<div id="fourth">
  <h4>Today's Weight</h4>

  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Number</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @weights.each do |weight| %>
        <tr>
          <td><%= weight.date %></td>
          <td><%= weight.number %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
